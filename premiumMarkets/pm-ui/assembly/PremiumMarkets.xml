<assembly xmlns="http://maven.apache.org/assembly/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.0.0 http://maven.apache.org/xsd/assembly-1.0.0.xsd">
	<id>PremiumMarkets_ass</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>	
		<fileSets>
			<fileSet>
				<directory>distrib/userParameterized</directory>
				<includes>
					<include>**/*</include>
				</includes>
				<outputDirectory>userParameterized</outputDirectory>
			</fileSet>
			<fileSet>
				<directory>distrib</directory>
				<includes>
					<include>db.properties</include>
					<include>list_test.txt</include>
					<include>COPYING</include>
					<include>README</include>
					<include>licenses.txt</include>
				</includes>
				<outputDirectory></outputDirectory>
			</fileSet>
			<fileSet>
				<directory></directory>
				<includes>
					<include>nativeOps.xml</include>
					<include>nativeIndicators.xml</include>
				</includes>
				<outputDirectory></outputDirectory>
			</fileSet>
			<fileSet>
				<directory>target/lib</directory>
				<outputDirectory>lib</outputDirectory>
			</fileSet>
			<fileSet>
				<directory>target</directory>
				<includes>
					<include>pm.jar</include>
				</includes>
				<outputDirectory></outputDirectory>
			</fileSet>
			<fileSet>
				<directory>shell</directory>
				<fileMode>0776</fileMode>
			</fileSet>
			<fileSet>
				<directory>icons</directory>
			</fileSet>
			<fileSet>
				<directory></directory>
				<excludes>
					<exclude>**/*</exclude>
				</excludes>
				<outputDirectory>autoPortfolioLogs</outputDirectory>
			</fileSet>
		</fileSets>
</assembly>
