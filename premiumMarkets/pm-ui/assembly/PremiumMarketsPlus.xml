<assembly xmlns="http://maven.apache.org/assembly/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.0.0 http://maven.apache.org/xsd/assembly-1.0.0.xsd">
	<id>PremiumMarkets_ass</id>
	<formats>
		<format>zip</format>
	</formats>
	<dependencySets>
		<dependencySet>
			<outputDirectory></outputDirectory>
			<includes>
				<include>
					com.finance.pm:pm-forecast:zip
				</include>
			</includes>
			<outputFileNameMapping></outputFileNameMapping>
			<unpack>true</unpack>
			<useProjectArtifact>true</useProjectArtifact>
 			<unpackOptions>
				<excludes>
					<exclude>**/META-INF/**</exclude>
					<exclude>pm-forecast.jar</exclude>
				</excludes>
			</unpackOptions>	
			<scope>runtime</scope>
		</dependencySet>
	</dependencySets>
	<includeBaseDirectory>false</includeBaseDirectory>	
		<fileSets>
			<fileSet>
				<directory>distrib</directory>
				<includes>
					<include>list_test.txt</include>
					<include>COPYING</include>
					<include>README</include>
					<include>licenses.txt</include>
				</includes>
				<outputDirectory></outputDirectory>
			</fileSet>
			<fileSet>
				<directory></directory>
				<includes>
					<include>nativeOps.xml</include>
					<include>nativeIndicators.xml</include>
					<include>nativeOpsEnhanced.xml</include>
				</includes>
				<outputDirectory></outputDirectory>
			</fileSet>
			<fileSet>
				<directory>target/lib</directory>
				<excludes>
					<exclude>PremiumMarketsForecast.zip</exclude>
					<exclude>pm-forecast.jar</exclude>
				</excludes>
				<outputDirectory>lib</outputDirectory>
			</fileSet>
			<fileSet>
				<directory>target</directory>
				<includes>
					<include>pm.jar</include>
				</includes>
				<outputDirectory></outputDirectory>
			</fileSet>
			<fileSet>
				<directory>shell</directory>
				<fileMode>0776</fileMode>
			</fileSet>
			<fileSet>
				<directory>icons</directory>
			</fileSet>
		</fileSets>
</assembly>
